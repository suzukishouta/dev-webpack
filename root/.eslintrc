{
	"env": {
		"browser": true,
		"es2021": true,
		"jquery": true
	},
	"extends": [
		"airbnb-base"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"jquery"
	],
	"rules": {
		"indent": [
				2,
				"tab"
		],
		"no-tabs": 0,
		"import/no-extraneous-dependencies":[
			"error", {
				"devDependencies": ["**/webpack.common.js","**/webpack.prod.js","**/webpack.dev.js", "**/src/js/**.js"],
				"peerDependencies": false
			}
		]
	}
}
